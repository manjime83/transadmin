{
  "name": "transadmin",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "prettier --write **/*.{ts,json,yaml} --ignore-path .gitignore --loglevel warn && npm-check-updates --upgrade",
    "clean": "bash scripts/clean.sh",
    "build": "bash scripts/build.sh",
    "deploy": "bash scripts/deploy.sh",
    "update": "bash scripts/update.sh",
    "update:watch": "watch -n -f \"functions/*.ts\" -r \"bash scripts/update.sh ${n}\""
  },
  "dependencies": {
    "cfn-response": "^1.0.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/cfn-response": "^1.0.3",
    "@types/node": "^13.9.5",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.648.0",
    "cli-watch": "^1.0.7",
    "npm-check-updates": "^4.1.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "printWidth": 120
  }
}
