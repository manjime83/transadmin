{
  "name": "transadmin",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "prettier --write **/*.{ts,json,yaml} --ignore-path .gitignore --loglevel warn && npm-check-updates --upgrade",
    "clean": "bash scripts/clean.sh",
    "build": "bash scripts/build.sh",
    "deploy": "bash scripts/deploy.sh",
    "update": "bash scripts/update.sh",
    "update:watch": "watch -n -f \"functions/*.ts\" -r \"bash scripts/update.sh ${n}\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "tslib": "^1.11.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.47",
    "@types/node": "^13.11.0",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.653.0",
    "cli-watch": "^1.0.7",
    "npm-check-updates": "^4.1.1",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "printWidth": 120
  }
}
