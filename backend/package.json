{
  "name": "transadmin",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "bash scripts/check-updates.sh && npm run lint",
    "lint": "prettier --write **/*.{ts,json,yaml} --ignore-path .gitignore --loglevel warn",
    "build": "tsc && sam validate && sam build && find .aws-sam -exec touch -t 8510260120 {} +",
    "deploy": "npm run build && sam deploy --no-fail-on-empty-changeset",
    "update": "tsc && bash scripts/update-function-code.sh"
  },
  "dependencies": {
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/gulp": "^4.0.6",
    "@types/node": "^13.9.3",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.646.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.3",
    "json": "^9.0.6",
    "npm-check-updates": "^4.0.6",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add ."
    }
  }
}
