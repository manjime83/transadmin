{
  "name": "transadmin",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "bash scripts/check-updates.sh && prettier --write **/*.{ts,json,yaml} --ignore-path .gitignore",
    "sam:build": "tsc && sam build && find .aws-sam -exec touch -t 8510260120 {} +",
    "sam:deploy": "npm run sam:build && sam deploy || exit 0"
  },
  "dependencies": {
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/node": "^13.9.2",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.643.0",
    "json": "^9.0.6",
    "npm-check-updates": "^4.0.4",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "always"
  }
}
