{
  "name": "transadmin",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "bash scripts/check-updates.sh && prettier --write **/*.{ts,json,yaml} --ignore-path .gitignore",
    "build": "tsc && sam build && find .aws-sam -exec touch -t 8510260120 {} +",
    "deploy": "npm run build && sam deploy || exit 0",
    "update": "tsc && bash scripts/update-function-code.sh"
  },
  "dependencies": {
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/node": "^13.9.2",
    "@types/uuid": "^7.0.2",
    "aws-sdk": "^2.643.0",
    "json": "^9.0.6",
    "npm-check-updates": "^4.0.4",
    "prettier": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "none"
  }
}
